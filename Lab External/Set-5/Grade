marks=float(input("enter the marks:"))
if(marks>100):
  print("Invalid Marks")
elif(marks>90):
    print("Grade S")
elif(marks>80):
    print("Grade A")
elif(marks>70):
    print("Grade B")
elif(marks>60):
    print("Grade C")
elif(marks>50):
    print("Grade D")
elif(marks >= 40):
    print("Grade E")
else:
    print("Grade F")
